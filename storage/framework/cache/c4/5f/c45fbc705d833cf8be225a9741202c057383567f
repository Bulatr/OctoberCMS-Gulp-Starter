                    class="shiping-value _shopaholic-order-field" 
                    id="shiping-value-{{ obShippingType.id }}"
                    data-group="order" 
                    data-shiping-id="shipping-type-{{ obShippingType.id }}"
                    name="shipping_price" 
                    value="{{obShippingType.price_value}}" 
                    data-shiping-price="{{obShippingType.price}}">
